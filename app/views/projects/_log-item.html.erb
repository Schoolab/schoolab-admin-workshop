<% if item.image? && item.description? %>

  <%= render "projects/log-item/description-image", item: item %>

<% elsif item.image? %>

  <%= render "projects/log-item/image", item: item %>

<% elsif item.description? %>

  <%= render "projects/log-item/description", item: item %>

<% else %>

  <%= render "projects/log-item/title", item: item %>

<% end %>
